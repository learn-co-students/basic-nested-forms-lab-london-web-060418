<%= form_for @recipe do |f| %>
  <p>Title: <%= f.text_field :title %></p>

   main ingredients:<br></br>
    <%= f.fields_for :ingredients do |ingredient| %>
      What:<%= ingredient.text_field :name %><br>
      How much:<%= ingredient.text_field :quantity %><br>
      <br>
    <% end %>
<br>
  <%= f.submit %>
<% end %>
